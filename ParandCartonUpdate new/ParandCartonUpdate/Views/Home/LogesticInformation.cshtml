</div>

<br />
<div id="PItable" class="content">

    <div class="clearfix text-black"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        adressf(0);
        $("#divpaletcount").hide()
    });
    function palet() {
        if ($("#palet").is(':checked', true)) {
            $("#divpaletcount").show()
        } else {
            $("#divpaletcount").hide()
        }
    }
</script>